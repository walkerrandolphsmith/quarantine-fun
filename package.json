    "init": "yarn && cd client && yarn && cd ../server && yarn",
    "build": "cd client && yarn && yarn build && cd ../server && yarn"
  },
  "dependencies": {
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
